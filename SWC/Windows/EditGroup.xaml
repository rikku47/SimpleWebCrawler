<Window x:Class="SWC.Windows.EditGroup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SWC.Windows"
        mc:Ignorable="d"
        Title="Gruppe bearbeiten" Height="100" Width="600" WindowStartupLocation="CenterScreen" MinWidth="600" MinHeight="100" MaxHeight="100">
    <Grid>
        <GroupBox Header="Gruppenname">
            <StackPanel>
                <TextBox Name="txtGroupname" Background="Azure"/>
                <Button Name="btnChangeGroupname" Content="Ändern"/>
            </StackPanel>
        </GroupBox>
    </Grid>
</Window>
